/**index.wxss**/
page {
  background-color: white;
}

button.form_button {
  background-color: transparent;
  padding: 0;
  margin: 0;
  display: inline;
  position: static;
  border: 0;
  padding-left: 0;
  padding-right: 0;
  border-radius: 0;
  font-size: 26rpx;
  
}


button.form_button::after {
  content: '';
  width: 0;
  height: 0;
  -webkit-transform: scale(1);
  transform: scale(1);
  display: none;
  background-color: transparent;
  
}

.navs {
  padding: 20rpx 50rpx;
  display: flex;
  align-items: flex-start;
  flex-flow: column wrap;
  flex-direction: row;
  height: 100%;
}

.nav {
  width: 25%;
  
}

.nav-icon {
  display: block;
  width: 60rpx;
  height: 60rpx;
  margin: 5rpx auto;
}

.nav-desc {
  display: block;
  margin: 5rpx;
}

.swiper {
  

  width: 100%;

  border-radius: 50rpx;

  overflow: hidden;

  margin: 20rpx auto;

 
  


}


.swiper image {
  width: 100%;
  height: 100%;
}

